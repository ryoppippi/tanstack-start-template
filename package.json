{
	"name": "@ryoppippi/tanstack-start-template",
	"type": "module",
	"private": true,
	"packageManager": "pnpm@10.7.1",
	"scripts": {
		"dev": "vinxi dev",
		"build": "nr build:css && nr build:app",
		"build:css": "unocss ./app/**/*.tsx --out-file ./app/uno.css",
		"build:app": "vinxi build",
		"start": "vinxi start",
		"lint": "eslint --cache .",
		"format": "nr lint --fix",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"prepare": "husky && touch ./app/uno.css"
	},
	"peerDependencies": {
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.71.5",
		"@tanstack/react-router": "^1.114.34",
		"@tanstack/react-start": "^1.114.34",
		"@unocss/reset": "66.1.0-beta.8",
		"clsx": "^2.1.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"vinxi": "^0.5.3"
	},
	"devDependencies": {
		"@antfu/ni": "^24.3.0",
		"@eslint-react/eslint-plugin": "^1.40.3",
		"@ryoppippi/eslint-config": "^0.3.7",
		"@ryoppippi/unocss-preset": "^2.0.1",
		"@std/collections": "npm:@jsr/std__collections@^1.0.10",
		"@tanstack/eslint-plugin-query": "^5.71.5",
		"@tanstack/eslint-plugin-router": "^1.114.29",
		"@types/bun": "latest",
		"@types/react": "^19.0.12",
		"@types/react-dom": "^19.0.4",
		"@unocss/cli": "66.1.0-beta.8",
		"@unocss/eslint-plugin": "66.1.0-beta.8",
		"@unocss/preset-attributify": "66.1.0-beta.8",
		"@vitest/ui": "^3.0.9",
		"babel-plugin-react-compiler": "19.0.0-beta-e993439-20250328",
		"eslint": "^9.23.0",
		"eslint-plugin-format": "^1.0.1",
		"eslint-plugin-react-compiler": "19.0.0-beta-e993439-20250328",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.0",
		"msw": "^2.7.3",
		"unocss": "66.1.0-beta.8",
		"unocss-transformer-alias": "^0.0.8",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.0.9"
	},
	"lint-staged": {
		"*": [
			"eslint --fix",
			"git add"
		]
	},
	"changelogithub": {
		"types": {
			"feat": {
				"title": "🚀 Features"
			},
			"fix": {
				"title": "🐞 Bug Fixes"
			},
			"perf": {
				"title": "🏎 Performance"
			},
			"docs": {
				"title": "📝 Documentation"
			},
			"test": {
				"title": "🧪 Tests"
			},
			"refactor": {
				"title": "💅 Refactors"
			},
			"ci": {
				"title": "🤖 CI"
			}
		}
	}
}
